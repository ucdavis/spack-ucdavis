prepend-path CMAKE_PREFIX_PATH {/share/apps/22.04/spack/opt/software/linux-ubuntu22.04-x86_64_v3/gcc-11.4.0/tabix-2013-12-16-nhdodty23wzksle7yqh2ptzzauann67b/.}
