module-whatis {Generic indexer for TAB-delimited genome position files}

proc ModulesHelp { } {
    puts stderr {Name   : tabix}
    puts stderr {Version: 2013-12-16}
    puts stderr {Target : x86_64_v3}
    puts stderr {}
    puts stderr {Generic indexer for TAB-delimited genome position files}
}

if {![info exists ::env(LMOD_VERSION_MAJOR)]} {
    module load perl/5.38.0-lfl2a73
    module load python/3.11.6-rsiojxh
} else {
    depends-on perl/5.38.0-lfl2a73
    depends-on python/3.11.6-rsiojxh
}
prereq perl/5.38.0-lfl2a73
prereq python/3.11.6-rsiojxh
conflict tabix

