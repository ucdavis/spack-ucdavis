#%Module1.0
# vim: syntax=tcl

module-whatis "Vcflib and related tools are the workhorses in bioinformatics for processing the VCF variant calling format."

set envname "vcflib-1.0.3"

prereq conda/base/latest

if { [module-info mode load] } {
	puts stdout "conda activate $envname 2> /dev/null; "
}

if { [module-info mode unload] } {
	puts stdout "conda deactivate 2> /dev/null; "
}
