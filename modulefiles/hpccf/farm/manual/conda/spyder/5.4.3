#%Module1.0
# vim: syntax=tcl

module-whatis "Spyder is a powerful scientific environment written in Python, for Python, and designed by and for scientists, engineers and data analysts."

set envname "spyder-5.4.3"

prereq conda/base/latest

if { [module-info mode load] } {
	puts stdout "conda activate $envname 2> /dev/null; "
}

if { [module-info mode unload] } {
	puts stdout "conda deactivate 2> /dev/null; "
}
