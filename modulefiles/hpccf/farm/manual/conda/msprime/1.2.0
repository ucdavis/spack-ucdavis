#%Module1.0
# vim: syntax=tcl

module-whatis "msprime can simulate ancestral histories for a sample of individuals, consistent with a given demography under a range of different models and evolutionary processes"

set envname "msprime-1.2.0"

prereq conda/base/latest

if { [module-info mode load] } {
	puts stdout "conda activate $envname 2> /dev/null; "
}

if { [module-info mode unload] } {
	puts stdout "conda deactivate 2> /dev/null; "
}
